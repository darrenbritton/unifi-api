{
  "name": "nodejs-minimal",
  "version": "2.0.1",
  "description": "A clean, simple nodejs starter",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src/*.js src/specs/*.js",
    "lint:fix": "npm run lint -- --fix",
    "start:dev": "nodemon --experimental-json-modules src/index.js",
    "start": "node --experimental-json-modules src/index.js",
    "test": "jest specs --watch",
    "test-ci": "jest specs"
  },
  "engines": {
    "node": ">=13.13.0"
  },
  "type": "module",
  "author": "Brian Gershon",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "babel-jest": "^27.4.2",
    "eslint": "^8.4.1",
    "eslint-plugin-jest": "^25.3.0",
    "jest": "^27.4.3",
    "nodemon": "^2.0.15"
  },
  "private": true,
  "dependencies": {
    "dotenv": "^10.0.0",
    "execa": "^6.0.0",
    "express": "^4.17.1",
    "swagger-ui-express": "^3.0.10"
  }
}
